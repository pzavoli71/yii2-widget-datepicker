{
    "name": "pzavoli71/yii2-widget-datepicker",
    "description": "Enhanced Yii2 wrapper for the bootstrap datepicker plugin (An updated version of kartik-v datetimepicker to enable dd/mm/yyyy hh:ii format datetime).",
    "keywords": ["yii2", "extension", "widget", "select2", "form", "date", "picker", "jquery", "plugin"],
    "homepage": "https://github.com/pzavoli71/yii2-widget-datepicker",
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Paride Zavoli",
            "email": "paride.zavoli71@gmail.com",
            "homepage": "http://fattodate.org/"
        }
    ],
    "require": {
        "kartik-v/yii2-krajee-base": ">=2.0.0"
    },
    "autoload": {
        "psr-4": {
            "kartik\\date\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.4.x-dev"
        }
    }
}
